{
  "name": "site-interactive-content-svelte-5",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build && pnpm minify-output",
    "minify-output": "find dist -name '*.js' -exec esbuild --minify --allow-overwrite '{}' '--outfile={}' ';'",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "lint": "prettier --check . && eslint .",
    "install-playwright-browsers": "playwright install",
    "e2e-test": "playwright test",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "@playwright/test": "^1.50.1",
    "@sveltejs/vite-plugin-svelte": "^4.0.0",
    "@tsconfig/svelte": "^5.0.2",
    "@types/luxon": "^3.4.2",
    "@types/node": "^20.12.8",
    "@types/qrcode": "^1.5.5",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "autoprefixer": "^10.4.19",
    "esbuild": "^0.20.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.45.1",
    "postcss": "^8.4.38",
    "prettier": "^3.1.1",
    "prettier-plugin-svelte": "^3.2.6",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "svelte-eslint-parser": "^0.43.0",
    "tailwindcss": "^3.4.3",
    "tslib": "^2.6.2",
    "typescript": "^5.5.4",
    "vite": "^5.4.4"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write"
    ],
    "*.ts": [
      "prettier --write"
    ],
    "*.svelte": [
      "prettier --write"
    ],
    "*.json": [
      "prettier --write"
    ],
    "*.yml": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@mdi/js": "^7.2.96",
    "@ohm-js/cli": "^1",
    "@square/svelte-store": "^1.0.18",
    "@tailwindcss/typography": "^0.5.9",
    "@xenova/transformers": "^2.17.2",
    "colorjs.io": "^0.4.5",
    "fraction.js": "^4.3.7",
    "idb-keyval": "^6.2.1",
    "lru-cache": "^10.2.2",
    "luxon": "^3.4.4",
    "ml-matrix": "^6.11.0",
    "ml-pca": "^4.1.1",
    "mobx": "^6.13.2",
    "ohm-js": "^16",
    "purescript-fns": "link:../purescript-fns",
    "qrcode": "^1.5.3"
  }
}
